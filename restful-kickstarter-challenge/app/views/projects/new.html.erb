<h1><u>Create a new Kickstarter Project Here</u></h1>

<%= form_for(@project) do |f| %>

  <%= f.text_field :title %>
  <%= f.label :title %><br>

  <%= f.text_area :description %>
  <%= f.label :description %><br>

  <%= f.text_field :owner %>
  <%= f.label :owner %><br>

  <%= f.number_field :owner_age %>
  <%= f.label :owner_age %><br>

  <%= f.submit %>
<% end %>
